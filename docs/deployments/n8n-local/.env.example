POSTGRES_PASSWORD=n8n_pass123

# Mitmproxy
MITMPROXY_WEB_PASSWORD=proxy123

# Logs
N8N_LOG_LEVEL=debug
N8N_LOG_OUTPUT=console
CODE_ENABLE_STDOUT=true
